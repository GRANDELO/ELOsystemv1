{
  "name": "stream-mock",
  "version": "2.0.5",
  "description": "Node stream mock module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/BastienAr/stream-mock.git",
  "author": "Bastien Arata <bastyen.a@gmail.com>",
  "license": "MIT",
  "keywords": [
    "stream",
    "mock",
    "test",
    "writable",
    "readable"
  ],
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:ts": "tsc",
    "build:doc": "run-s doc:*",
    "doc:1-clean": "rimraf docs",
    "doc:2-build": "typedoc src",
    "doc:3-sanitize": "rexreplace \"$PWD/\" '' --quiet docs/**/*.html",
    "clean": "rimraf lib coverage .nyc_output yarn-error.log test-result.xml lint-result.xml",
    "lint": "tslint src/**/*.ts",
    "test": "jest --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^12.0.10",
    "@types/sinon": "^7.0.11",
    "husky": "^2.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "prettier-package-json": "^2.1.0",
    "rexreplace": "^4.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typedoc-plugin-sourcefile-url": "^1.0.4",
    "typescript": "^3.4.2"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  }
