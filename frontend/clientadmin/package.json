{
  "name": "client",
  "version": "0.1.0",
  "private": true,

  "main": "electron.js",
  "build": {
    "appId": "com.bazeadmin.desktop",
    "productName": "bazeadmin",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "electron.js"
    ],
    "mac": {
      "icon": "public/logo.ico",
      "target": "dmg"
    },
    "win": {
      "icon": "public/logo.ico",
      "target": "nsis"
    },
    "linux": {
      "category": "Utility",
      "target": ["AppImage"],
      "icon": "public/logo512.png"
    }
  },

  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "file-saver": "^2.0.5",
    "gridfs-stream": "^1.1.1",
    "jwt-decode": "^4.0.0",
    "mongoose": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "multer-gridfs-storage": "^5.0.2",
    "qrcode.react": "^4.1.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-modal": "^3.16.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.25.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.8.0",
    "redux": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "react-scripts start & electron .",
  "electron:build": "electron-builder",
  "electron-build": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "electron": "^34.0.1"
  }
}
